        <!--</repository>-->
        <!--<snapshotRepository>-->
        <!--<id>archiva.snapshots</id>-->
        <!--<name>Internal Snapshot Repository</name>-->
        <!--<url>http://nexus.gethehe.com/nexus/content/repositories/snapshots/</url>-->
        <!--</snapshotRepository>-->
    </distributionManagement>
</project>